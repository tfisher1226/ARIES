			<xs:element name="creationDate" type="xs:dateTime" minOccurs="0" nullable="true" />
			<xs:element name="lastUpdate" type="xs:dateTime" minOccurs="0" nullable="true" />
		</xs:sequence>
	</xs:complexType>
	
	<!--
		Role element
		************
	-->
	
	<xs:element name="role" type="sgiusa:Role" />
	<xs:complexType name="Role">
		<xs:sequence>
			<xs:element name="id" type="xs:long" minOccurs="0" />
