			<xs:element name="emailAddress" type="common:EmailAddress" />
			<xs:element name="streetAddress" type="common:StreetAddress" minOccurs="0" nullable="true" />
			<xs:element name="homePhone" type="common:PhoneNumber" minOccurs="0" nullable="true" />
			<xs:element name="cellPhone" type="common:PhoneNumber" minOccurs="0" nullable="true" />
			<xs:element name="roles" type="sgiusa:Role" maxOccurs="unbounded" nullable="true" />
			<xs:element name="permissions" type="sgiusa:Permission" minOccurs="0" maxOccurs="unbounded" nullable="true" />
