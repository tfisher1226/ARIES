#!/bin/sh

. ./thebes-env


#
# Local settings
#

LOCAL_WORKSPACE=/c/workspace/ARIES2

LOCAL_ECOS_HOME=$LOCAL_WORKSPACE/ecos

LOCAL_ADMIN_DS_FILE=$LOCAL_ECOS_HOME/deploy/$HOST/$ADMIN_DS_FILE

LOCAL_ADMIN_JMS_FILE=$LOCAL_ECOS_HOME/deploy/$HOST/$ADMIN_JMS_FILE

LOCAL_ECOS_EAR_FILE=$LOCAL_ECOS_HOME/ecos-ear/target/$ECOS_EAR_FILE

USER=sgiusa

REMOTE_ADDRESS=$USER@$HOST

REMOTE_PATH=$REMOTE_ADDRESS:$JBOSS_DEPLOYMENT_HOME

scp $LOCAL_ADMIN_JMS_FILE \
	$LOCAL_ECOS_EAR_FILE \
	$REMOTE_PATH

