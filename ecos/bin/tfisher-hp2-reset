#!/bin/sh

. ./tfisher-hp2-env

rm -f $JBOSS_DEPLOYMENT_HOME/$ECOS_EAR_FILE.*
#rm -f $JBOSS_DEPLOYMENT_HOME/$ECOS_DS_FILE.*
#rm -f $JBOSS_DEPLOYMENT_HOME/$ECOS_JMS_FILE.*
rm -f $JBOSS_DEPLOYMENT_HOME/$ADMIN_DS_FILE.*
rm -f $JBOSS_DEPLOYMENT_HOME/$ADMIN_JMS_FILE.*

chmod 644 $JBOSS_DEPLOYMENT_HOME/$ECOS_EAR_FILE
#chmod 644 $JBOSS_DEPLOYMENT_HOME/$ECOS_DS_FILE
#chmod 644 $JBOSS_DEPLOYMENT_HOME/$ECOS_JMS_FILE
chmod 644 $JBOSS_DEPLOYMENT_HOME/$ADMIN_DS_FILE
chmod 644 $JBOSS_DEPLOYMENT_HOME/$ADMIN_JMS_FILE

touch $JBOSS_DEPLOYMENT_HOME/$ECOS_EAR_FILE.dodeploy
#touch $JBOSS_DEPLOYMENT_HOME/$ECOS_DS_FILE.dodeploy
#touch $JBOSS_DEPLOYMENT_HOME/$ECOS_JMS_FILE.dodeploy
touch $JBOSS_DEPLOYMENT_HOME/$ADMIN_DS_FILE.dodeploy
touch $JBOSS_DEPLOYMENT_HOME/$ADMIN_JMS_FILE.dodeploy

chmod 644 $JBOSS_DEPLOYMENT_HOME/$ECOS_EAR_FILE.dodeploy
#chmod 644 $JBOSS_DEPLOYMENT_HOME/$ECOS_DS_FILE.dodeploy
#chmod 644 $JBOSS_DEPLOYMENT_HOME/$ECOS_JMS_FILE.dodeploy
chmod 644 $JBOSS_DEPLOYMENT_HOME/$ADMIN_DS_FILE.dodeploy
chmod 644 $JBOSS_DEPLOYMENT_HOME/$ADMIN_JMS_FILE.dodeploy

