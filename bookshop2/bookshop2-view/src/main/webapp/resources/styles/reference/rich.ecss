/*
 * Picklist related
 */

.rich-list-picklist,
.rich-picklist-body {
	margin: 0px;
	padding: 0px;
	border-spacing: 0px;
	border-collapse: collapse;
}

.rich-picklist-body {
	display: block;
}

.rich-list-picklist {
	display: block;
}

