/*
 * Menu related
 */

.rich-menu-item,
.rich-menu-group {
	padding-top: 2px;
	padding-bottom: 2px;
	padding-left: 1px;
	padding-right: 0px;
	border: 1px solid transparent;
}

/*
*/

.rich-menu-item-hover,
.rich-menu-group-hover {
	padding-top: 2px;
	padding-bottom: 2px;
	padding-left: 1px;
	padding-right: 0px;
	background-color: #deedf3;
	border-color: #a8d8eb;
	border: 1px solid #a8d8eb;
	color: blue;
}

.rich-menu-item-enabled {
}

.rich-menu-item-disabled {
	color: #808080;
}

.rich-menu-item-icon,
.rich-menu-item-label,
.rich-menu-group-icon,
.rich-menu-group-label {
	width: 0px;
	margin: 0px;
	padding: 0px;
	text-align: left;
	vertical-align: middle;
}

.rich-menu-item-label-disabled {
	
}

.rich-menu-list-border {
	
}

.rich-menu-list-bg {
	
}

.rich-ddmenu-label {
	
}

.rich-ddmenu-label-select {
	background-color: #deedf3;
	border-color: #a8d8eb;
}

