#!/bin/sh

. ./tfisher-hp2-env

rm -f $JBOSS_DEPLOYMENT_HOME/$NAM_EAR_FILE*
rm -f $JBOSS_DEPLOYMENT_HOME/$NAM_JMS_FILE*
rm -f $JBOSS_DEPLOYMENT_HOME/$NAM_DS_FILE*
rm -f $JBOSS_DEPLOYMENT_HOME/$ADMIN_JMS_FILE*
rm -f $JBOSS_DEPLOYMENT_HOME/$ADMIN_DS_FILE*

cp $NAM_EAR_HOME/target/$NAM_EAR_FILE $JBOSS_DEPLOYMENT_HOME
cp $NAM_HOME/deploy/$HOST/$NAM_JMS_FILE $JBOSS_DEPLOYMENT_HOME
cp $NAM_HOME/deploy/$HOST/$NAM_DS_FILE $JBOSS_DEPLOYMENT_HOME
cp $NAM_HOME/deploy/$HOST/$ADMIN_JMS_FILE $JBOSS_DEPLOYMENT_HOME
cp $NAM_HOME/deploy/$HOST/$ADMIN_DS_FILE $JBOSS_DEPLOYMENT_HOME

. ./tfisher-hp2-reset

